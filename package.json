    "@material-ui/lab": "^4.0.0-alpha.60",
    "@mui/icons-material": "^5.0.4",
    "@mui/material": "^5.0.2",
    "axios": "^0.21.4",
    "cross-env": "^7.0.3",
    "next": "^11.1.3",
    "next-pwa": "^5.3.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-touch-drag-slider": "^2.1.3",
    "styled-components": "^5.3.3",
    "styled-icons": "^10.45.0",
    "styled-media-query": "^2.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
